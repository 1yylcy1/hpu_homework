{
    
    "workbench.colorTheme": "Visual Studio Dark",
    "terminal.integrated.defaultProfile.windows": "Command Prompt",
    "code-runner.runInTerminal": true,
    "files.encoding": "gbk",
    "grunt.autoDetect": "on",
    "code-runner.preserveFocus": true,//used to focus mouse
    "code-runner.ignoreSelection": true,

    "settingsSync.ignoredExtensions": [
    ],
    "code-runner.executorMap": {
        "cpp": "cd $dir && g++ -std=c++14 -I \"D:\\IDE_code_practice\\vs_code\\c++_files\\include\" $fileName -o .\\exe\\$fileNameWithoutExt.exe && .\\exe\\$fileNameWithoutExt.exe",
        //"objective-c": "cd $dir && gcc -framework Cocoa $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        //"cpp": "cd $dir && g++ -c $fileName -o exe/$fileNameWithoutExt.exe && 'exe/$fileNameWithoutExt.exe'",
        //"cpp": "cd $dir && g++ $fileName -o .\\exe\\$fileNameWithoutExt.exe && .\\exe\\$fileNameWithoutExt.exe",
    "code-runner.clearPreviousOutput": false,
    "files.autoSave": "afterDelay"
},
"files.associations": {
    "iostream": "cpp",
    "array": "cpp",
    "atomic": "cpp",
    "*.tcc": "cpp",
    "cctype": "cpp",
    "clocale": "cpp",
